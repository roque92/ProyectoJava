<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gctcompanion">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".CrearBajaActivity"></activity>
        <activity android:name=".CrearRepresentateActivity" />
        <activity android:name=".CrearVendedorActivity" />
        <activity android:name=".CrearClienteActivity" />
        <activity android:name=".CrearCasoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DetalleCasosActivity" />
        <activity android:name=".CasosActivity" />
        <activity android:name=".PrincipalActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".RecuperarPasswordActivity" />
    </application>

</manifest>